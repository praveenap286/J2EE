public class Main
{
	public static void main(String[] args) {
		 
		String s = "PraVeeN"; 
		String str = " ";
	    for(int j=0;j<s.length();j++){
	    if(Character.isLowerCase(s.charAt(j)))
	    str=str+Character.toUpperCase(s.charAt(j));
	    else
	    str=str+Character.toLowerCase(s.charAt(j));
	    }
		System.out.println(str);
	}
}
